<?xml version="1.0"?>
<launch>

  <!--#>>>>TODO: LAUNCH A NODE TO PUBLISH AN STATIC TRANSFORMATION BETWEEN /map and /WORLD WITH A FREQ 100-->
  <node name="map" pkg="tf" type="static_transform_publisher" args="1 0 0 0 0 0 1 /map /world 100"/>
  <!--#>>>>TODO: LAUNCH A NODE TO SET A DESIRED POSITION-->
  <node name="tiago_set_position" pkg="tiago_control" type="tiago_set_pos" output="screen"/>
  <!--#>>>>TODO:LAUNCH TURTLE CONTROL NODE. AS PART OF THIS NODE LOAD THE YAML FILE WITH THE GAIN PARAMETERS (SEE rvizConfigs/turtle_gains.yaml)-->
  <node name="tiago_control" pkg="tiago_control" type="tiago_control">
    <rosparam command="load" file="$(find tiago_control)/launch/rvizConfigs/turtle_gains.yaml" />
  </node>
</launch>
